    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh filename="package://four_dof_manipulator/meshes/third_link.STL" />
      </geometry>
    </collision>
  </link>
  
  <!--Joint 3-->
  <joint name="joint_3" type="revolute">
    <origin
      xyz="0.3 0 0.124"
