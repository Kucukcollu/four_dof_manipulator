      <mass
        value="1.20713988278658" />
      <inertia
        ixx="0.00387509235916713"
        ixy="1.08751366378478E-10"
        ixz="2.91710497028995E-17"
        iyy="0.00304586202042799"
        iyz="1.8833505378577E-18"
        izz="0.00589439778201493" />
